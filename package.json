{
  "name": "sig-js",
  "version": "0.5.3",
  "description": "high-level reactive-style programming in javascript",
  "type": "commonjs",
  "main": "./index.js",
  "exports": {
    "import": "./index.mjs",
    "require": "./index.js"
  },
  "scripts": {
    "build": "otr build",
    "lint": "otr lint",
    "format": "otr format",
    "test": "otr test",
    "release": "otr release",
    "checks": "yarn lint && yarn build && yarn test"
  },
  "author": "justinvdm",
  "license": "MIT",
  "devDependencies": {
    "@oftherivier/tools": "0.1.39"
  },
  "oftherivier": {
    "type": "es5",
    "src": "index.js",
    "bundleName": "sig"
  },
  "renovate": {
    "extends": [
      "github>oftherivier/tools"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/justinvdm/sig.git"
  },
  "keywords": [
    "reactive",
